<p id="notice"><%= notice %></p>

<h1>States</h1>

<table>
  <thead>
    <tr>
      <th>State name</th>
      <th>State url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @states.each do |state| %>
      <tr>
        <td><%= link_to @state, state.state_name %></td>
        <td><%= link_to @state_url, state.state_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
